{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Jwt": {
    "Key": "SOME_RANDOM_KEY_DO_NOT_SHARE",
    "Issuer": "http://auth.venturi.com",
    "ExpireDays": 30
  },
  "Lockout": {
    "LockoutReleaseServiceIntervalInMilliseconds": 1000,
    "LockoutDurationInMilliseconds": 1000,
    "MaximumAllowedFailedSignInAttempts": 5
  }
}
